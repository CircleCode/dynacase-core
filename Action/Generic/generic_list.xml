                    var firstAbstract=$('#cellmain div:first');

                    if (firstAbstract) {
                        var startX=$('#cellmain').position().left;
                        var percentage = widthStr.substr(0, index);
                        var divWidth = $("div#main").width();

                        var delta= $('#cellmain div:first').outerWidth(true) - $('#cellmain div:first').width();
