#rewrite rules for download file 
<FilesMatch "^.+$">
  Order Allow,Deny
  Allow from All
</FilesMatch>

RewriteEngine On
RewriteCond %{REQUEST_URI} !/guest\.php$
RewriteRule ^([^/]*)/([0-9]*)/([^/]*)/([^/]*)/(.*)$ ../index.php?app=FDL&action=EXPORTFILE&docid=$1&vid=$2&attrid=$3&index=$4&filename=$5&$6 [L,QSA]
