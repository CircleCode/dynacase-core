#!/bin/bash



if [ -z "$wpub" ]; then
    echo "Undefined or empty wpub environment variable!"
    exit 1
fi

if [ -z "$pgservice_core" ]; then
    echo "Undefined or empty pgservice_core!"
    exit 1
fi


$wpub/wsh.php --api=destroyFamily --famid=FORUM
# no control error because can be alreday deleted

exit 0
