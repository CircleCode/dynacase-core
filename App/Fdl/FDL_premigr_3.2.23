#!/bin/bash

if [ -z "$pgservice_core" ]; then
	echo "Undefined or empty pgservice_core!"
	exit 1
fi

PGSERVICE="$pgservice_core" psql --set ON_ERROR_STOP=on -f - <<'EOF'
delete from doc where id in (502, 503, 513, 506, 507);
EOF
RET=$?
if [ $RET -ne 0 ]; then
    echo "Error removing some mask."
    exit $RET
fi

